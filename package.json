{
  "name": "frontend-core",
  "version": "0.1.0",
  "description": "It's simple bare bones repository for frontend React developers using BEM",
  "private": true,
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack-dev-server --mode development --open",
    "dev": "cross-env NODE_ENV=development webpack --mode development",
    "build": "cross-env NODE_ENV=production webpack --mode production"
  },
  "browserslist": "> 0.2%, not dead, last 2 versions",
  "keywords": [
    "BEM",
    "html",
    "pug",
    "css",
    "scss",
    "js",
    "ts",
    "tsx",
    "javascript",
    "typesctipt",
    "react",
    "webpack"
  ],
  "author": "NZMinusDev <developer.n.zaytsev@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@prettier/plugin-pug": "^1.5.1",
    "@typescript-eslint/eslint-plugin": "^4.1.0",
    "@typescript-eslint/parser": "^4.1.0",
    "eslint": "^7.9.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "prettier": "^2.1.1",
    "webpack-cli": "^3.3.12"
  },
  "dependencies": {
    "webpack": "^4.44.1",
    "typescript": "^4.0.2"
  }
}
